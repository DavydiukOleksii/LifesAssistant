    <!-- add icon-->
    <Icon Id="icon.ico" SourceFile="icon.ico" />
    <Property Id="ARPPRODUCTION" Value="icon.ico" />

    <!--add install bg-->
    <WixVariable Id="WixUIBannerBmp" Value="Images\installer_top-banner.bmp" />
